<h1>Party</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.Name }}</td>
        </tr>
        <tr>
            <th>Start</th>
            <td>{{ entity.Start|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>End</th>
            <td>{{ entity.End|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Place</th>
            <td>{{ entity.Place }}</td>
        </tr>
        <tr>
            <th>Host</th>
            <td>{{ entity.Host }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('party') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('party_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('party_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
